<html>
<head>
    <title>Ingredients</title>
</head>
<body>
<h1>Ingredients</h1>
<table border="1">
    <tr><th>Ingredient ID</th><th>Name</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><%= result[i].ingredient_id%></td>
        <td><%= result[i].ingredient_name %></td>
    </tr>
    <% } %>
</table>
</body>

</html>